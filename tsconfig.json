{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "types": [
        "core-js",
        "hammerjs",
        "jasmine",
        "node",
        "protractor",
        "selenium-webdriver"
     ]
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "exclude": [
        "node_modules",
        "phonegap"
  ]
}
